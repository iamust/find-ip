{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "noEmit": true
  },
  "include": ["src"]
}
